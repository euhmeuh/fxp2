        <child id="title/obj_press_start" />
        
        <!-- Environment -->
        <child id="title/map_ground" />
        <child id="title/cha_character" />

        <!-- Background -->
        <child id="title/obj_mask" />
        <child id="title/obj_thumbnail" />

    </object>
    
