    <object id="zon_title" type="Fxp.Zone">
    
        <properties>
            <rect x="0" y="0" w="2000" h="1000" />
        </properties>

        <node id="config">
            <key id="starting_point_x" type="integer">0</key>
            <key id="starting_point_y" type="integer">0</key>
        </node>

        <!-- Title -->
        <child id="title/obj_logo" />
        
